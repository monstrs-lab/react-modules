{
  "name": "@monstrs/react-cookie",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.8",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
